{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "ignoreCommand": "echo 'Ignoring cache for monorepo Drizzle fix'"
}